<Window x:Class="lesson3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lesson3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MinHeight="400" MinWidth="700">
    <Grid  Background="DarkGray">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="116px"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="116px"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image Source="images/zadanieIcon.jpg" Margin="5px"/>
        <Label Name="HeaderLabel" Content="Вычисление корней" Grid.Column="1" FontSize="30"
               HorizontalAlignment="Right" VerticalAlignment="Center"/>
        <Frame Name="Book" Grid.Row="1" Grid.ColumnSpan="2" NavigationUIVisibility="Hidden"
               Source="/lesson3;component/pages/firstPagexaml.xaml"/>
    </Grid>
</Window>
